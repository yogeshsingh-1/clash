{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon ./dist/index.js",
    "watch": "tsc -w",
    "dev": "concurrently  \"npm run watch\" \"npm run server\" \"npm run watch-views\"",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/src",
    "build": "tsc && npm run copy-files",
    "watch-views": "nodemon --watch src/views -e ejs -x \"npm run copy-files\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "bullmq": "^5.62.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "nodemailer": "^7.0.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "copyfiles": "^2.4.1",
    "typescript": "^5.9.3"
  }
}
